import { Socials } from "socials"
import { Users } from "users"

model UsersSocials {
    userId    Int
    socialId  Int
    url       String
    createdAt DateTime

    user   Users   @relation(fields: [userId], references: [id])
    social Socials @relation(fields: [socialId], references: [id])

    @@id([userId, socialId])
    @@map("usersSocials")
}
